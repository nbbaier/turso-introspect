{
	"name": "turso-introspect",
	"version": "0.1.0",
	"description": "A CLI tool to introspect the database schema of a Turso/libsql database.",
	"keywords": [
		"turso",
		"libsql",
		"sqlite",
		"schema",
		"introspection",
		"cli"
	],
	"homepage": "https://github.com/nbbaier/turso-introspect#readme",
	"bugs": {
		"url": "https://github.com/nbbaier/turso-introspect/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nbbaier/turso-introspect.git"
	},
	"license": "MIT",
	"author": "Nico Baier <nico.baier@gmail.com>",
	"type": "module",
	"bin": {
		"turso-introspect": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"start": "bun run src/index.ts",
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "echo \"Error: no test specified\" && exit 1",
		"prepublishOnly": "npm run build",
		"check": "biome check .",
		"fix": "biome check --write --unsafe .",
		"typecheck": "tsc --noEmit"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@types/bun": "^1.3.5",
		"@types/diff": "^8.0.0",
		"tsdown": "^0.19.0-beta.2"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@libsql/client": "^0.15.15",
		"chalk": "^5.6.2",
		"commander": "^14.0.2",
		"diff": "^8.0.2"
	}
}
